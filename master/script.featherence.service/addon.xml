	<import addon="xbmc.json" version="6.0.0"/>
	<import addon="xbmc.python" version="2.24.0"/>
	<!--<import addon="script.module.requests" version="2.3.0" optional="true"/>
	<import addon="script.common.plugin.cache" version="2.5.5" optional="true"/>-->
  </requires>
  <extension point="xbmc.python.script" library="default.py"/>
