		<control type="button" id="405" description="ברירת מחדל">
		  <include>SideMenuCommon</include>
		  <include condition="String.IsEqual(System.Language,Hebrew)">ButtonR1</include>
		  <include condition="!String.IsEqual(System.Language,Hebrew)">ButtonL1</include>
		  <font>size28</font>
		  <label>$LOCALIZE[571] $INFO[Window(home).Property(TEMP)] $INFO[Window(home).Property(Button.Name)]</label>
		  <onclick>RunScript(script.featherence.service,,?mode=235)</onclick>
		  <visible>Window.IsVisible(CustomHomeCustomizer.xml) + !String.IsEmpty(Window(home).Property(Button.ID)) + !IntegerGreaterThan(Window(home).Property(Button.ID),99) + !Window.IsVisible(Custom1139.xml)</visible>
		  <visible>String.IsEqual(Window(home).Property(TEMP),icon) | String.IsEqual(Window(home).Property(TEMP),background)</visible>
		  <visible>Window.IsVisible(FileBrowser.xml)</visible>
		</control>
		
		<control type="button" id="406" description="העדפת תצוגה: ">
		  <include>SideMenuCommon</include>
		  <include condition="String.IsEqual(System.Language,Hebrew)">ButtonR1</include>
		  <include condition="!String.IsEqual(System.Language,Hebrew)">ButtonL1</include>
		  <font>size28</font>
		  <label>$VAR[406label]</label>
		  <onclick>Skin.ToggleSetting(51over50)</onclick>
		  <onclick>Dialog.Close(filebrowser)</onclick>
		  <visible>Window.IsVisible(FileBrowser.xml)</visible>
		</control>
		
