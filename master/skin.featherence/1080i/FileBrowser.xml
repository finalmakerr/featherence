		  <include condition="!String.IsEqual(System.Language,Hebrew)">ButtonL1</include>
		  <font>size28</font>
		  <label>119</label>
		  <visible>Window.IsVisible(FileBrowser.xml) + Control.IsEnabled(415)</visible>
		</control>
		<control type="radiobutton" id="416" description="Horizontal Flip">
		  <include>SideMenuCommon</include>
		  <include condition="String.IsEqual(System.Language,Hebrew)">ButtonR1</include>
		  <include condition="!String.IsEqual(System.Language,Hebrew)">ButtonL1</include>
		  <font>size28</font>
